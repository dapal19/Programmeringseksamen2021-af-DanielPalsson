<h1> Her ser du en oversigt over dine varer: </h1>
<div>
    <table class="table table-bordered">
    <tr> 
    <th scope="col"> Pris </th>
    <th scope="col">Kategori</th>
    <th scope="col">beskrivelse</th>
    <th scope="col">Billede</th>
    </tr>
    <% items.forEach(function(allItems) { %>
    <tr> 
    <td> <%=allItems.pris %> </td>
    <br>
    <td> <%=allItems.kategori %> </td>
    <br>
    <td> <%=allItems.beskrivelse %></td>
    <br>
    <td> <%=allItems.billede %> </td>
    </tr>
    <% }) %>
    </table>
</div>
